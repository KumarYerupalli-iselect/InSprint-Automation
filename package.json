{
  "name": "nightwatch-test-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./tests_output/ && rimraf ./tests/build && rimraf ./tests/reports/*.html && rimraf ./tests/screenshots/",
    "compile": "cd ./tests && tsc --outDir ./build/",
    "regressionTests:uat": "npx nightwatch ./tests/build/functionalTests/Regression --reporter html-reporter.js -c ./tests/configFiles/nightwatch.conf.js",
    "runSingleTest": "npx nightwatch ./tests/build/functionalTests/SprintQ2_13_14/PS-1098.js --reporter=html -c ./tests/configFiles/nightwatch.conf.js",
    "regressionTests:prod": "npx nightwatch ./tests/build/functionalTests/Regression --reporter html-reporter.js -c ./tests/configFiles/nightwatch.conf.js -e PROD",
    "testSafari:browserstack": "npx nightwatch -c ./tests/configFiles/nightwatch.browserstack.conf.js ./build/functionalTests -a quickTestCheck -e browserstack.safari",
    "allure": "allure generate ./allure-results --clean && allure open",
    "test": "npm run clean && npm run compile && npm run runSingleTest && npm run allure"
  },
  "keywords": [],
  "author": "Kumar Raja Yerupalli (Peter)",
  "license": "ISC",
  "devDependencies": {
    "@types/nightwatch": "^2.3.14",
    "@types/node": "^18.11.9",
    "chromedriver": "^108.0.0",
    "dot-env": "^0.0.1",
    "fs": "^0.0.1-security",
    "geckodriver": "^3.0.1",
    "handlebars": "^4.7.7",
    "nightwatch": "^2.5.1",
    "nightwatch-allure": "^1.2.0",
    "nightwatch-html-reporter": "^2.0.6",
    "node-random-name": "^1.0.1",
    "path": "^0.12.7",
    "pretty-error": "^4.0.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.2.0",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "csv-parse": "^5.3.3"
  }
}
